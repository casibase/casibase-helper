{
  "branches": ["master"],
  "plugins": [
    "@semantic-release/commit-analyzer",
    "@semantic-release/release-notes-generator",
    "@semantic-release/git",
    [
      "@semantic-release/github",
      {
        "assets": [
          {"path": "release/*.exe", "label": "Windows Installer"},
          {"path": "release/*.dmg", "label": "macOS Installer"},
          {"path": "release/*.AppImage", "label": "Linux AppImage"}
        ]
      }
    ]
  ]
}
